{
  "name": "@chatapp/native-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/react-native",
      "**/react-native/**",
      "**/react-native-*",
      "**/react-native-*/**",
      "**/mobx-devtools",
      "**/mobx-devtools/**",
      "**/mobx-devtools-mst",
      "**/mobx-devtools-mst/**",
      "**/mobx-react",
      "**/mobx-react/**"
    ]
  },
  "scripts": {
    "build:env": "sh ./scripts/build-env.sh",
    "build:e2e:ios": "detox build --configuration ios.sim.release",
    "build:e2e:android": "detox build --configuration android.emu.release",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "watchman:clean": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android",
    "rename": "sh ./scripts/rename.sh",
    "podspec": "sh ./scripts/podspec.sh",
    "test": "jest",
    "test:e2e": "yarn build:e2e:ios && yarn test:e2e:ios",
    "test:e2e:ios": "detox test --configuration ios.sim.release",
    "test:e2e:android": "detox test --configuration android.emu.release",
    "prepare": "patch-package && if [ -f ./scripts/postinstall.sh ]; then sh ./scripts/postinstall.sh; fi"
  },
  "dependencies": {
    "@chatapp/foo": "*",
    "@react-native-community/async-storage": "1.2.0",
    "jsc-android": "241213.0.0",
    "lodash": "4.17.11",
    "metro-config": "^0.53.1",
    "mobx": "5.9.0",
    "mobx-devtools-mst": "0.9.18",
    "mobx-react": "5.4.3",
    "mobx-state-tree": "3.11.0",
    "react": "16.8.4",
    "react-native": "0.59.0",
    "react-native-code-push": "5.5.2",
    "react-native-config": "0.11.7",
    "react-native-firebase": "5.2.3",
    "react-native-navigation": "2.13.1",
    "react-native-sentry": "0.42.0",
    "react-native-ueno-css-modules": "1.1.0",
    "react-native-version-number": "0.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@types/core-decorators": "^0.20.0",
    "@types/date-fns": "^2.6.0",
    "@types/enzyme": "3.9.0",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.9",
    "@types/lodash": "4.14.122",
    "@types/mobx-devtools-mst": "0.9.0",
    "@types/react-native": "0.57.38",
    "@types/react-native-navigation": "2.0.0",
    "@types/react-test-renderer": "16.8.1",
    "@types/styled-components": "^4.1.12",
    "babel-jest": "24.3.1",
    "babel-plugin-styled-components": "^1.10.0",
    "detox": "10.0.12",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "jest": "24.3.1",
    "jest-haste-map": "^24.0.0",
    "jest-resolve": "^24.1.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "mobx-devtools": "0.9.20",
    "node-sass": "4.11.0",
    "patch-package": "6.0.5",
    "postinstall-prepare": "1.0.1",
    "react-dom": "16.8.4",
    "react-native-rename": "2.4.1",
    "react-test-renderer": "16.8.4",
    "ts-jest": "24.0.0",
    "typescript": "3.3.3333"
  },
  "detox": {
    "test-runner": "jest",
    "specs": "e2e",
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/chatapp.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/chatapp.xcworkspace -scheme chatapp -configuration Release -sdk iphonesimulator -derivedDataPath ios/build ONLY_ACTIVE_ARCH=YES -quiet -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
        "type": "android.emulator",
        "name": "Pixel_2_API_28"
      }
    }
  }
}
